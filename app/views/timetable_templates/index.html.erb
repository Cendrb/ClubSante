<p id="notice"><%= notice %></p>

<h1>Listing Timetable Templates</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @timetable_templates.each do |timetable_template| %>
      <tr>
        <td><%= link_to 'Zobrazit', timetable_template %></td>
        <td><%= link_to 'Upravit', edit_timetable_template_path(timetable_template) %></td>
        <td><%= link_to 'Odstranit', timetable_template, method: :delete, data: { confirm: 'Jste si jisti?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Timetable template', new_timetable_template_path %>
