$("body").append("<%= escape_javascript(render partial: "tickets/ticket_selector", locals: { tickets: @data[:tickets], therapy: @data[:target_therapy] }) %>");
$('#ticket_selector').dialog(
	{
        resizable: false,
        modal: true,
        width:'1200px',
		close: function(event, ui)
        {
            $(this).dialog('destroy').remove();
        }
  });
