<!DOCTYPE html>
<html>
	<head>
		<title>ClubSante</title>
		<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
		<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
		<%= csrf_meta_tags %>
	</head>
	<body>
		<div id="container">
			<div id="page">
				<div id="header">
					<%= render partial: "users/current_user_status" %>
				</div>
				<div id="content">
					<%= yield %>
				</div>
			</div>
		</div>
	</body>
</html>
