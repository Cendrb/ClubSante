<p id="notice"><%= notice %></p>

<h1>Listing Timetable Modifications</h1>

<table>
  <thead>
    <tr>
      <th>Calendar</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @timetable_modifications.each do |timetable_modification| %>
      <tr>
        <td><%= timetable_modification.calendar_id %></td>
        <td><%= link_to 'Show', timetable_modification %></td>
        <td><%= link_to 'Edit', edit_timetable_modification_path(timetable_modification) %></td>
        <td><%= link_to 'Destroy', timetable_modification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Timetable modification', new_timetable_modification_path %>
